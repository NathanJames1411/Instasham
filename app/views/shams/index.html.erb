<div class="shams-wrapper row">
  <% @shams.each do |sham| %>
    <div class="sham">
      <div class="sham-head">
        <div class="name">
          Nathan James
        </div>
      </div>
      <div class="image center-block">
        <%= link_to (image_tag sham.image.url(:medium), class:'img-responsive'), sham_path(sham) %>
      </div>
      <p class="caption">
        <%= sham.caption %>
      </p>
    </div>
  <% end %>
</div>
